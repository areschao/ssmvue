<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chao.ssmvue.base.security.mapper.LogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.chao.ssmvue.base.business.log.entity.Log">
    <result column="id" property="id" />
    <result column="create_time" property="createTime" />
        <result column="login_name" property="loginName" />
        <result column="role_name" property="roleName" />
        <result column="opt_content" property="optContent" />
        <result column="client_ip" property="clientIp" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_time,
        login_name, role_name, opt_content, client_ip
    </sql>

</mapper>
